Pav_y1
{
 	// Mandatory entries
        type                surfaceFieldValue;
        libs                ("libfieldFunctionObjects.so");
        writeControl        timeStep;
        log                 true;
        writeFields         no;
        surfaceFormat	    none; 
	operation	    areaAverage;
/*        operation           weightedAreaIntegrate;*/
/*	weightField         phi;*/
	fields		    (p UMean p_rgh TMean sbMean s1Mean s2Mean s3Mean s4Mean s5Mean s6Mean s7Mean s8Mean s9Mean LMAMean);

        regionType          sampledSurface;
	name 		    av_y1;
        sampledSurfaceDict
	{

		    type            cuttingPlane;
		    planeType       pointAndNormal;
		    pointAndNormalDict
		    {
		        point   (0.0 0.1 0.0);
		        normal  (0 1 0);
		    }
	}

}

